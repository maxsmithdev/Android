<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <Button
        android:id="@+id/button_qr_generate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="QRCode Generate" />

    <Button
        android:id="@+id/button_code_128_generate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Barcode Generate (code 128)" />

    <Button
        android:id="@+id/button_qr_surfaceview_scanner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="QRCode Scanner [SurfaceView]" />

    <Button
        android:id="@+id/button_code_128_surfaceview_scanner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Barcode Scanner (code 128) [SurfaceView]" />
</LinearLayout>
